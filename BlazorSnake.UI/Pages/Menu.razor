@page "/"
@inject NavigationManager NavManager

<p><button @onclick="__GoToNewGame">New game</button></p>
<p><button @onclick="__GoToSettings">Settings</button></p>

@code
{
    private void __GoToNewGame()
    {
        NavManager.NavigateTo("/BlazorSnake/snake");
    }

    private void __GoToSettings()
    {
        NavManager.NavigateTo("/BlazorSnake/settings");
    }
}
